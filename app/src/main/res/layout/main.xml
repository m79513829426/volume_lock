<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@color/menuNavigation1colorPrimary"
	android:orientation="horizontal"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	>

	<TextView
		android:id="@+id/et1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Активная защита"/>

	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent">

	</RelativeLayout>

	<Button
		android:id="@+id/mb1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="V"
		android:background="@color/menuNavigation1Button3Bg"
		/>

	<Button
		android:id="@+id/mb2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="SDK"
		android:background="@color/menuNavigation1Button3Bg"
		/>

</LinearLayout>

